<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="com.github.gradusnikov.eclipse.assistai.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="always"
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.github.gradusnikov.eclipse.assistai.preferences.SamplePreferencePage"
            id="com.github.gradusnikov.eclipse.assistai.preferences.SamplePreferencePage"
            name="Assit AI">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.github.gradusnikov.eclipse.assistai.preferences.PreferenceInitializer">
      </initializer>
   </extension>
